{
  "name": "Paypal App",
  "author": {
    "name": "Zendesk",
    "email": "support@zendesk.com"
  },

  "parameters": [
    {
      "name": "paypal_api_username",
      "type": "text",
      "default": "",
      "secure": true,
      "required": true
    },
    {
      "name": "paypal_api_password",
      "type": "text",
      "default": "",
      "secure": true,
      "required": true

    },
    {
      "name": "paypal_api_signature",
      "type": "text",
      "default": "",
      "secure": true,
      "required": true

    },
    {
      "name": "paypal_sandbox",
      "type": "checkbox"
    }
  ],
  
  "domainWhitelist": ["api-3t.sandbox.paypal.com", "api-3t.paypal.com"],

  "defaultLocale": "en",
  "private": true,
  "location": "ticket_sidebar",
  "version": "1.0",
  "frameworkVersion": "1.0"
}
